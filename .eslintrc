{
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "env": {
      "node": true,
      "es6": true,
      "browser": true,
      "jest": true
    },
    "rules": {
      "no-console": "off",
      "prettier/prettier": "error",
      "max-classes-per-file": ["error", 100]
    }
  }